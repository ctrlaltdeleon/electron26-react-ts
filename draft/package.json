    "cross-env": "^7.0.3",
    "electron": "^4.2.12",
    "electron-builder": "22.3.2",
    "foreman": "^3.0.1",
    "prettier": "2.2.1",
    "react-contexify": "^4.1.1",
    "react-scripts": "^5.0.1",
