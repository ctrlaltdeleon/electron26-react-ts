    "testcafe": "^1.8.1",
    "testcafe-browser-provider-electron": "^0.0.14"
  },
  "jest": {
    "collectCoverageFrom": [
      "**/*.{js,}",
      "!**/node_modules/**",
      "!**/vendor/**"
    ],
    "coveragePathIgnorePatterns": [
      "src/index.js",
      "src/test/e2e/App.e2e.js",
      "src/sonar-project.js",
      "src/test"
